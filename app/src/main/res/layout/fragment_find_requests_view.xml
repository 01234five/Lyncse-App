<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.FindRequestsViewFragment">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="To see inquiries, press below"
            android:textAlignment="center"
            android:textSize="20sp" />
        <Button
            android:id="@+id/buttonFindRequestsView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="10dp"
            android:backgroundTint="#FFFFFF"
            android:text="View Inquiries"
            android:textColor="#FF03DAC5"
            android:textSize="20sp"
            app:strokeColor="#FF03DAC5" />



    </LinearLayout>

</FrameLayout>